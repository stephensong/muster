{
  "name": "@dws/muster-observable",
  "version": "6.7.0",
  "description": "Muster Observable",
  "module": "./esm/index.js",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "/dist",
    "/esm"
  ],
  "repository": {
    "type": "git",
